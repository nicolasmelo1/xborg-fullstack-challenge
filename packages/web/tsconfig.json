{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@xborg/config/tsconfig.nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts",
    "**/*.mjs"
  ],
  "exclude": ["node_modules"]
}
